// Faça um algoritmo que receba um número e mostre uma mensagem caso este número seja maior que 10.
programa {
  funcao inicio() {
    real nmr = 0
    enquanto (nmr <= 10)
    {
      escreva ("Digite um número: ")
      leia (nmr)
      limpa()
    }
    escreva("Esse número é maior que 10")
  }
}
